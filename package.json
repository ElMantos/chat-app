    "react-hook-form": "^6.12.2",
    "react-native": "0.63.3",
    "react-native-cli": "^2.0.1",
    "react-native-vector-icons": "^7.1.0",
    "react-redux": "^7.2.2",
    "react-router-native": "^5.2.0",
    "redux": "^4.0.5",
